mkdir -p exp/results/dpgmm
head eval/abx/result/exp/dpgmm/*/kl/abx.txt | awk 'BEGIN {printf "Method\tABX_average\n"} /abx\.txt/ {printf gensub(/.*\/(.*)\/(.*).post\/(cos|edit|kl)\/abx\.txt/, "\\2_\\3", "g", $2)"\t"} /average/ {print $5"\t"}' | sort -k1.27,1.28 -k1.36,1.37g | tee exp/results/dpgmm/abx_kl.txt
